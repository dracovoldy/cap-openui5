{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "com.limscloud.app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.32.0"
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"dependencies": {
			"minUI5Version": "1.52.9",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.limscloud.app.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"viewType": "JS",
				"routerClass": "sap.m.routing.Router",
				"viewPath": "com.limscloud.app.view",
				"controlId": "appView",
				"controlAggregation": "pages",
				"transition": "slide"
			},
			"routes": [
				{
					"name": "InitialRoute",
					"pattern": "",
					"greedy": false,
					"target": [
						"LoginPage"
					]
				},
				{
					"name": "LoginPage",
					"pattern": "LoginPage",
					"greedy": false,
					"target": [
						"LoginPage"
					]
				},
				{
					"name": "Dashboard",
					"pattern": "Dashboard/{itemId}",
					"greedy": false,
					"target": [
						"Dashboard"
					]
				},
				{
					"name": "dashboardTiles",
					"pattern": "dashboardTiles",
					"greedy": false,
					"target": [
						"dashboardTiles"
					]
				},
				{
					"name": "dashboardTasks",
					"pattern": "dashboardTasks",
					"greedy": false,
					"target": [
						"dashboardTasks"
					]
				},
				{
					"name": "CU01",
					"pattern": "CU01",
					"greedy": false,
					"target": [
						"CU01"
					]
				},
				{
					"name": "CU05",
					"pattern": "CU05",
					"greedy": false,
					"target": [
						"CU05"
					]
				},
				{
					"name": "SU01",
					"pattern": "SU01",
					"greedy": false,
					"target": [
						"SU01"
					]
				},
				{
					"name": "SU05",
					"pattern": "SU05",
					"greedy": false,
					"target": [
						"SU05"
					]
				}
			],
			"targets": {
				"SU01": {
					"viewType": "XML",
					"parent": "Dashboard",
					"controlId": "pageContainer",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "CreateUsers",
					"viewLevel": 3
				},
				"SU05": {
					"viewType": "XML",
					"parent": "Dashboard",
					"controlId": "pageContainer",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "ModifyUsers",
					"viewLevel": 3
				},
				"CU01": {
					"viewType": "XML",
					"parent": "Dashboard",
					"controlId": "pageContainer",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "CreateCustomers",
					"viewLevel": 3
				},
				"CU05": {
					"viewType": "XML",
					"parent": "Dashboard",
					"controlId": "pageContainer",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "ModifyCustomers",
					"viewLevel": 3
				},
				"Dashboard": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "dashboard",
					"viewLevel": 1
				},
				"dashboardTiles": {
					"viewType": "XML",
					"parent": "Dashboard",
					"controlId": "pageContainer",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "dashboardTiles",
					"viewLevel": 2
				},
				"dashboardTasks": {
					"viewType": "XML",
					"parent": "Dashboard",
					"controlId": "pageContainer",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "dashboardTasks",
					"viewLevel": 2
				},
				"LoginPage": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "login",
					"viewLevel": 1
				},
				"LandingPage": {
					"viewType": "XML",
					"viewName": "LandingPage"
				},
				"Scope": {
					"viewType": "XML",
					"viewName": "Scope"
				},
				"Infrastructure": {
					"viewType": "XML",
					"viewName": "Infrastructure"
				},
				"Development": {
					"viewType": "XML",
					"viewName": "Development"
				},
				"Data": {
					"viewType": "XML",
					"viewName": "Data"
				},
				"Others": {
					"viewType": "XML",
					"viewName": "Others"
				},
				"POCM": {
					"viewType": "XML",
					"viewName": "POCM"
				},
				"FinalDashBoard": {
					"viewType": "XML",
					"viewName": "FinalDashBoard"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}