<mvc:View controllerName="cap.estimate.controller.dashboardTiles" displayBlock="true" height="100%" xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc">
	<App id="dashboardTilesApp">
		<pages>
			<Page id="page2" showHeader="false" class="sapUiSizeCompact">
				<content>
					<MessageStrip showIcon="true"
						  type="Information"
						  text="Quick access to tiles by T-Code, use searchbar to run Tiles with T-Code"
						  class="sapUiTinyMargin"/>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Create New System User" subheader="Singe creation"
						press="_navSU01">
						<TileContent footer="SU01">
							<ImageContent src="sap-icon://create"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Maintain System Users" subheader="Mass View/Modify"
						press="_navSU05">
						<TileContent footer="SU05">
							<ImageContent src="sap-icon://user-edit"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Create Customer" subheader="Single creation" press="_navCU01">
						<TileContent footer="CU01">
							<ImageContent src="sap-icon://home-share"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Maintain Customers" subheader="Mass View/Modify" press="_navCU05">
						<TileContent footer="CU05">
							<ImageContent src="sap-icon://customer"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Receive Materials from Customer" subheader="Mass Transfer"
						press="_navMM01">
						<TileContent footer="MM01">
							<ImageContent src="sap-icon://dimension"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Create Task Unassigned" subheader="Unitary task"
						press="_navTT01">
						<TileContent footer="TT01">
							<ImageContent src="sap-icon://activity-items"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Create Task for User" subheader="Unitary task"
						press="_navTT02">
						<TileContent footer="TT02">
							<ImageContent src="sap-icon://activity-individual"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Assign Task to User" subheader="Unitary task"
						press="_navTT03">
						<TileContent footer="TT03">
							<ImageContent src="sap-icon://activity-assigned-to-goal"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Launch Task" subheader="Tasks for user" 
					press="_navTL01">
						<TileContent footer="TL01">
							<ImageContent src="sap-icon://physical-activity"/>
						</TileContent>
					</GenericTile>
					<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Monitor Tasks" subheader="All tasks"
					press="_navTM01">
						<TileContent footer="TM01">
							<ImageContent src="sap-icon://electrocardiogram"/>
						</TileContent>
					</GenericTile>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>